{
    "permissions": ["contextMenus", "clipboardRead"],
    "manifest_version": 2,
    "name": "tutorial extension 3",
    "version": "3.0",
    "content_scripts": [
        {
           "matches": [
                "<all_urls>"
           ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "chika.png"
    },
    "icons": {
        "128": "chika.png"
    }
}
